{
  "name": "hapkido-student-3",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:staging": "ng run app:build:staging --stats-json",
    "build:prod": "ng build --prod",
    "build:test": "ng run app:build:test --stats-json",
    "build:dev": "ng run app:build:dev",
    "local": "ng serve",
    "staging": "ng run app:serve:staging",
    "prod": "ng run app:serve:production",
    "dev": "ng run app:serve:dev",
    "start:staging": "ng run app:serve:staging",
    "start:test": "ng run app:serve:test",
    "start:dev": "ng run app:serve:dev",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cy:run:ci:no-record": " $(npm bin)/cypress run --config baseUrl=http://localhost:8000",
    "cy:ci:no-record": "npm run build && run-p --race serve:ci cy:run:ci:no-record",
    "serve:ci": "angular-http-server --path www -a localhost -p 8000",
    "copy:capacitor": "./node_modules/ionic/bin/ionic capacitor copy android",
    "open:capacitor": "./node_modules/ionic/bin/ionic capacitor open android",
    "device:local": "npm run build && npm run copy:capacitor && npm run open:capacitor",
    "device:staging": "npm run build:staging && npm run copy:capacitor && npm run open:capacitor",
    "device:dev": "npm run build:dev && npm run copy:capacitor && npm run open:capacitor",
    "device:prod": "npm run build:prod && npm run copy:capacitor && npm run open:capacitor",
    "lighthouse:run": "lighthouse http://localhost:4200 --output-path=./reports/lighthouse/lighthouse-results.json --output=json",
    "lighthouse:ci": "lighthouse-ci http:localhost:4200 --score=-1 --report",
    "lighthouse:report": "./node_modules/.bin/ts-node ./reports/lighthouse/lighthouse-reporter.ts",
    "analyse-package-size": "webpack-bundle-analyzer ./www/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~10.0.0",
